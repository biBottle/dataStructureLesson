class Solution {
public:
    ListNode* rotateRight(ListNode* head, int k) {
        struct ListNode* p, *q;
        int i,n = 1;
        if(head==NULL)
        {
            return NULL;
        }
        for(p=head; p->next!=NULL; p=p->next)
        {
            n++;
        }
        k %= n;
        q = p;
        for(p=head,i=0; n-k-1!=i; p=p->next,i++);
        q->next = head;
        head = p->next;
        p->next=NULL;
        return head;
    }
};
